<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT DATABASE (TABLE+)>
<!ATTLIST DATABASE
	name CDATA #REQUIRED
	datatype CDATA #REQUIRED
>
<!ELEMENT TABLE (COLUMNS, USERS*, INSERT?)>
<!ATTLIST TABLE
	name CDATA #REQUIRED
>
<!ELEMENT COLUMNS (COLUMN+)>
<!ELEMENT COLUMN (#PCDATA)>
<!ATTLIST COLUMN
	type CDATA #REQUIRED
	primary_key CDATA #IMPLIED
	index CDATA #IMPLIED
	not_null CDATA #IMPLIED
	foreign_key CDATA #IMPLIED
	refTable CDATA #IMPLIED
	refColumn CDATA #IMPLIED
>
<!ELEMENT USERS (USER)>
<!ELEMENT USER (#PCDATA)>
<!ATTLIST USER
	grant CDATA #REQUIRED
>
<!ELEMENT INSERT (ROWS+)>
<!ELEMENT ROWS (ROWDATA+)>
<!ELEMENT ROWDATA (#PCDATA)>
<!ATTLIST ROWDATA
	name CDATA #REQUIRED
>