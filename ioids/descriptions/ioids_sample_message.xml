<ioids-message>
    <ioidsevent>
        bla bla
        <event>
            bla bla
        </event>
    </ioidsevent>
    
    <relations>
        <relation type="parent">
            <event encoding="idmef">
                <idmef>
                    blabla
                </idmef>
            </event>
            <extension type="syslog">
                <syslog>
                    <timestamp>2005-10-13 11:50</timestamp>
                    blabla
                </syslog>
            </extension>
        </relation>
    
        <relation type="reference">
            <event encoding="idmef">
                <idmef>
                    other blabla
                </idmef>
            </event>
            <extension type="ioids">
                <ioidsevent>
                    even other blabla
                </ioidsevent>
            </extension>
        </relation>
    </relations>
    
</ioids-message>